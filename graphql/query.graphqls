type Query {
    # Cards
    getMTGCards: [MTG_Card!]!
    getMTGFilters: MTG_Filter_Entries!
    # Decks
    getMTGDecks(deckID: ID): [MTG_Deck!]!
    # Card Packages
    getMTGCardPackages(cardPackageID: ID): [MTG_CardPackage!]!
    # Tags
    tags: [Tag!]!
    cardTags: [CardTag!]!
    deckTags: [DeckTag!]!
    tag(id: ID!): Tag
}
