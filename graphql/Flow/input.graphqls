"""
Used to save the deck builder zones and their children.
"""
input FlowZoneInput {
    ID: ID!
    name: String!
    position: PositionInput!
    width: Float!
    height: Float!
    cardChildren: [ID!]!
    zoneChildren: [ID!]!
}

"""
Used to save the position of an entity in the deck builder.
"""
input PositionInput {
    x: Float!
    y: Float!
}
