"""
Represents a zone in the deck builder.
"""
type FlowZone {
    ID: ID!
    name: String!
    position: Position!
    width: Float!
    height: Float!
    cardChildren: [ID!]!
    zoneChildren: [ID!]!
}

"""
Represents the position of an entity in the deck builder.
"""
type Position {
    x: Float!
    y: Float!
}
