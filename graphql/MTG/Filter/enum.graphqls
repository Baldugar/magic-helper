"""
Three-state boolean used for filter entries.
"""
enum TernaryBoolean {
    TRUE
    FALSE
    UNSET
}

"""
Sortable fields for card lists.
"""
enum MTG_Filter_SortBy {
    NAME
    CMC
    RARITY
    COLOR
    TYPE
    SET
    RELEASED_AT
}

"""
Sort direction.
"""
enum MTG_Filter_SortDirection {
    ASC
    DESC
}
