type MTG_Deck {
    ID: ID! @goTag(key: "json", value: "_key")
    list: MTG_CardListType!
    name: String!
    cardFrontImage: String
    cards: [MTG_DeckCard!]!
    zones: [FlowZone!]!
    type: DeckType!
    ignoredCards: [String!]!
}

type MTG_DeckCard {
    card: MTG_Card!
    count: Int!
    position: Position!
    mainOrSide: MainOrSide!
    deckCardType: MTG_DeckCardType!
    phantoms: [Phantom!]!
}

type Phantom {
    position: Position!
    ID: ID!
}
