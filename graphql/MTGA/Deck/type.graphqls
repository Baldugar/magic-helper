type MTGA_Deck {
    ID: ID! @goTag(key: "json", value: "_key")
    name: String!
    cardFrontImage: String
    cards: [MTGA_DeckCard!]!
    zones: [FlowZone!]!
    type: DeckType!
}

type MTGA_DeckCard {
    card: MTGA_Card!
    count: Int!
    position: Position!
    mainOrSide: MainOrSide!
    deckCardType: MTGA_DeckCardType!
    phantoms: [Position!]!
}
