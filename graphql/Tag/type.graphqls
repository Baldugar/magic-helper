"""
A tag that can annotate cards or decks.
"""
interface Tag {
    ID: ID! @goTag(key: "json", value: "_key")
    name: String!
    description: String
    myRating: UserRating
}

"""
A tag intended for annotating cards.
"""
type CardTag implements Tag {
    ID: ID! @goTag(key: "json", value: "_key")
    name: String!
    description: String
    myRating: UserRating
}
